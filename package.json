{
  "name": "b3wallet",
  "version": "0.5.1",
  "author": "b3hr4d",
  "license": "MIT",
  "description": "Wallet and System Canister for the B3wallet on the Internet Computer",
  "keywords": [
    "wallet",
    "bitcoin",
    "ethereum",
    "blockchain",
    "internet computer",
    "icp",
    "dfinity"
  ],
  "scripts": {
    "autobuild": "bash ./scripts/AUTOBUILD.sh",
    "start": "npx serve@latest out",
    "dfx:start": "bash ./scripts/start.sh",
    "dfx:pull": "dfx deps pull",
    "dfx:install": "yarn dfx:identity && sh ./scripts/install.sh",
    "dfx:identity": "dfx deps deploy internet_identity",
    "dfx:stop": "dfx stop",
    "predeploy": "sh ./scripts/predeploy.sh",
    "deploy": "dfx deploy",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "generate": "dfx generate",
    "load-wasm:wallet": "npx ts-node ./scripts/load-wasm.wallet.ts",
    "load-wasm:system": "npx ts-node ./scripts/load-wasm.system.ts",
    "copy-wasm": "yarn build:wallet && ts-node ./scripts/copy-wasm.ts",
    "wasm-hash": "npx ts-node ./scripts/wasm-hash.ts",
    "dev": "npx next dev",
    "build": "dfx generate && npx next build",
    "env": "node -e \"console.log(require('dotenv').config({ path: '.env' }))\""
  },
  "devDependencies": {
    "@ic-reactor/store": "^0.4.1",
    "@types/node": "20.8.10",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "18.2.36",
    "@types/react-beautiful-dnd": "^13",
    "@types/react-is": "^18",
    "@types/semver": "^7.5.0",
    "@xpd/tailwind-3dtransforms": "^1.0.3",
    "assert": "^2.1",
    "buffer": "^6.0.3",
    "compressorjs": "^1.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "events": "^3.3.0",
    "node-fetch": "^3.3",
    "pem-file": "^1.0.1",
    "postcss": "^8",
    "react-dropzone": "^14.2",
    "semver": "^7.5",
    "stream-browserify": "^3.0",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "5.3.2",
    "util": "^0.12"
  },
  "dependencies": {
    "@dfinity/agent": "^0.20",
    "@dfinity/auth-client": "^0.20",
    "@dfinity/candid": "^0.20",
    "@dfinity/identity": "^0.20",
    "@dfinity/identity-secp256k1": "^0.20",
    "@dfinity/principal": "^0.20",
    "@ic-reactor/react": "^0.4.2",
    "@peculiar/webcrypto": "1.4.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "autoprefixer": "^10.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "next": "^14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-is": "^18.2.0",
    "recharts": "^2.10.1",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  }
}
